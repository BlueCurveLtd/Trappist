<Page x:Class="DemoApp.Views.NavigatedWith"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:bedrock="http://ingroupe.innovation.bedrock"
      xmlns:local="clr-namespace:DemoApp.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="NavigatedWith"
      bedrock:ViewModelLocator.AutoWireViewModel="True">

    <Grid>
        <TextBlock Text="{Binding NavigationType}" FontSize="20" VerticalAlignment="Center" HorizontalAlignment="Center" />
    </Grid>
</Page>
